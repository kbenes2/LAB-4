
package com.company;
import java.util.Scanner;



public class Main {

    public static float convert(float f)
    {
        float c;

        c = (f-32)*5/9;
        System.out.println(c);
        return(c);
    }


   public static void main(String[] args) 
   {
	    Scanner scan = new Scanner(System.in);
	    float f = scan.nextInt();
	    convert(f);
    }
}
